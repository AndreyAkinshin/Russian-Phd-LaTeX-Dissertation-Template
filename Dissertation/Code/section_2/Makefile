run:
	@# - распараллеливание вычисления эволюции системы (8 процессов) 
	@#   для выбранного диапазона интенсивности оттока фотонов 
	@# - построение графика
	@mpirun -np 8 python3 main.py config.py && python3 make_plot.py out

clean:
	@# удаление выходных данных и файла с графиком
	@rm -rf out plot3d.html
